{
  "scripts": {
    "start": "NODE_ENV=production node dist",
    "build": "tsc -p tsconfig.build.json",
    "migration": "knex migrate:latest",
    "docker-build-prod": "docker build -t brainflash-api:latest -f ./docker/prod/Dockerfile .",
    "dev": "NODE_ENV=development nodemon --watch \"src/**/*.ts\" --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts",
    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "test:watch": "NODE_ENV=test jest --config ./jest.config.js --watch",
    "test:ci": "NODE_ENV=test docker-compose up -d database && jest --config ./jest.config.js --ci --coverage --forceExit",
    "lint": "eslint --ext .ts \"src/**/*\"",
    "format": "prettier --write \"src/**/*\"",
    "typecheck": "tsc --noEmit",
    "version": "conventional-changelog -p emojis -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "schema": "TS_NODE_PROJECT=tsconfig.json graphql-codegen --config ./src/api/apollo/codegen.yml"
  },
  "_moduleAliases": {
    "@root": ".",
    "@api": "dist/api",
    "@app": "dist/app",
    "@infrastructure": "dist/infrastructure",
    "@tools": "dist/tools",
    "@errors": "dist/errors",
    "@core": "dist/core",
    "@tests": "dist/tests"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "^1.17.9",
    "@types/aws-lambda": "^8.10.63",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-enforces-ssl": "^1.1.1",
    "@types/faker": "^4.1.12",
    "@types/helmet": "0.0.48",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.7",
    "@types/redis": "^2.8.27",
    "@types/sharp": "^0.26.0",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "conventional-changelog-cli": "^2.1.0",
    "conventional-changelog-core": "^4.2.0",
    "conventional-changelog-emojis": "^3.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
